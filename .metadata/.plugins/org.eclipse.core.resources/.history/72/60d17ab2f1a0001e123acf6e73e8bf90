server:
  port: 9085
  
spring:
  application:
    name: CONFIG-SERVER
    
    
  cloud:
    config:
      server:
        git:
          uri: https://github.com/SurajGaur97/micro-services-config
          clone-on-start: true
          
eureka:
  instance:
    prefer-ip-address: true
  client:
    fetch-registry: true
    register-with-eureka: true
    service-url:
      defaultZone: http://localhost:9080/eureka
      